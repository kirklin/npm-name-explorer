    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.6",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.2",
    "iso-639-1": "^3.1.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.31",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/mdi": "^1.1.67",
    "@iconify-json/tabler": "^1.1.117",
    "@intlify/unplugin-vue-i18n": "^4.0.0",

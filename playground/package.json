{
  "name": "npm-name-explorer-examples",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "description": "npm-name-explorer-examples",
  "keywords": [],
  "main": "src/main.ts",
  "scripts": {
    "dev": "run-p dev:playground dev:mock",
    "dev:playground": "vite",
    "dev:mock": "pnpm --filter npm-name-explorer-api dev",
